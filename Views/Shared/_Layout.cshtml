            .contact h2::before {
                content: '';
                position: absolute;
                bottom: -5px;
                left: 0;
                width: 50px;
                height: 2px;
                background: #f00;
            }

        .contact {
            width: calc(35% - 60px);
            margin-right: 0 !important;
        }

            .contact .info {
                position: relative;
            }

